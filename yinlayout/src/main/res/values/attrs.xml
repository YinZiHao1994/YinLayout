<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--是否多选-->
    <attr name="multiple" format="boolean" />
    <attr name="child_checkable" format="boolean" />
    <attr name="child_check_state_cancelable" format="boolean" />

    <declare-styleable name="CheckableGroupFlowLayout">
        <attr name="multiple" />
        <attr name="child_checkable" />
        <attr name="child_check_state_cancelable" />
    </declare-styleable>

    <declare-styleable name="CommonCheckableGroup">
        <attr name="multiple" />
        <attr name="child_checkable" />
        <attr name="child_check_state_cancelable" />
    </declare-styleable>


    <!--卫星菜单展开的子item的总角度-->
    <attr name="total_angle" format="integer" />

    <declare-styleable name="MoonMenu">
        <attr name="total_angle" />
    </declare-styleable>


    <declare-styleable name="SideMenuLayout">
        <attr name="menu_trigger" format="float" />
        <attr name="click_event_range" format="integer" />
    </declare-styleable>

</resources>